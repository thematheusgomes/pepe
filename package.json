  "devDependencies": {
    "serverless": "^2.62.0",
    "serverless-offline": "^8.2.0",
    "serverless-plugin-resource-tagging": "^1.1.1",
    "serverless-python-requirements": "^5.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Boomcredit/pepe.git"
  },
