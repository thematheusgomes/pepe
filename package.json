{
  "name": "pepe",
  "version": "1.0.0",
  "description": "### Clone Repository",
  "main": "index.js",
  "scripts": {
    "remove:dev": "yarn install; serverless remove --stage dev",
    "deploy:dev": "yarn install; serverless deploy --stage dev --verbose",
    "deploy:tools": "yarn install; serverless deploy --stage tools --verbose",
    "deploy:function:dev": "yarn install; serverless deploy function -f pepe --stage dev",
    "deploy:function:tools": "yarn install; serverless deploy function -f pepe --stage tools",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
